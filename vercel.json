
{
    "version": 2,
    "builds": [
        {
            "src": "main_synchronous.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.12" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python main_synchronous.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt"
}
